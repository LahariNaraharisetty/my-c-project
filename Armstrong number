#include <stdio.h>
void main()
{
    int n,i,a,r;
    printf("enter any number:");
    scanf("%d",&n);
    i=n;
    while(i>0)
    {
        r=i%10;
        a=a+r*r*r;
        i=i/10;  
    }
    if(a==n)
      printf("The given number is a armstrong number");
    else
      printf("The given number is not a armstrong number");
     
}
